          if [ -n "" ] ; then echo COMPILING da_tracing.f90 WITH OMP ; fi ; \
  time mpif90 -f90=ifort -o da_tracing.o -c -O3 -ip -fp-model precise -w -ftz -align all -fno-alias -FR -convert big_endian    -r8        -real-size `expr 8 \* 8` -i4 da_tracing.f ; \
